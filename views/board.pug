extends layout

block content
    //Can use same class names for color stuff, but id's need to be unique
    div(id='addCardButton') Add Card
    div(id='saveButton') Save
    br
    if boardData
        div(id='container') !{boardData}
    else
        div(id='container')
            div(id="parentDiv")
    div(id="editBackground" style="color: black; width: 100%; height: 100%;")

    script(src='scripts/cardScript.js')
    script(src='scripts/boardSaving.js')